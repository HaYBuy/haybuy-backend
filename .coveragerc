[run]
source = app
omit =
    */venv/*
    */.venv/*
    */tests/*
    */__pycache__/*
    */migrations/*
    */.pytest_cache/*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
